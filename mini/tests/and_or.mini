program Foo:
    declare (i, k) integer;

    for i := 0 to 10 do

    	if i = 0 then
	   output "i is zero";
	fi;

	if i > 0 then
	   output "i", i, "greater zero";
	fi;

	if i > 3 and i < 5 then
	   output "i", i, "between 3 and 5";
	fi;

	if i >= 3 and i <= 5 then
	   output "i", i, "between 3 and 5 inclusive";
	fi;
    end for;
end program Foo;
